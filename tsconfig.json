{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2017",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "build",
        "types": ["node", "pg", "discord.io", "official-discord"],
        "typeRoots": ["node_modules/@types", "./types"]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "**/@types.ts"
    ]
}
