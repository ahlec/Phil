{
  "name": "PhilTheYeti",
  "version": "14.3.2",
  "database_version": "11",
  "description": "A Discord bot made specifically for the Official Hijack (Hiccup Haddock/Jack Frost) Discord server",
  "main": "main.js",
  "author": "ahlec",
  "engines": {
    "node": ">=12.x",
    "yarn": "^1.6.4"
  },
  "scripts": {
    "start": "heroku local web",
    "build": "tsc",
    "clean": "rm -rf ./build",
    "lint": "eslint \"./{src,scripts}/**/*.{js,ts}\"",
    "run-bot": "node build/src/main.js",
    "db-migrate": "node scripts/db-migrate.js"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "chalk": "^2.4.2",
    "chrono-node": "1.3.5",
    "discord.io": "https://github.com/ahlec/discord.io.git#gateway_v6",
    "fuse.js": "^3.4.2",
    "lodash": "^4.17.11",
    "module-alias": "^2.2.2",
    "moment-timezone": "0.5.23",
    "node-cache": "^4.2.0",
    "pg": "8.3.3",
    "semver": "^6.2.0",
    "youtube-node": "https://github.com/ahlec/youtube-node.git#fix-callback-typing"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "@types/moment-timezone": "^0.5.4",
    "@types/node": "^12.6.2",
    "@types/node-cache": "^4.1.1",
    "@types/pg": "^7.14.4",
    "@types/semver": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "countries-and-timezones": "1.0.1",
    "countryjs": "1.8.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "postgrator": "^4.0.1",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahlec/Phil"
  },
  "keywords": [
    "bot",
    "discord",
    "hijack",
    "node",
    "hiccup haddock",
    "jack frost",
    "rotg",
    "httyd",
    "dreamworks"
  ],
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5"
  },
  "_moduleAliases": {
    "@data": "build/data",
    "@phil": "build/src"
  }
}
